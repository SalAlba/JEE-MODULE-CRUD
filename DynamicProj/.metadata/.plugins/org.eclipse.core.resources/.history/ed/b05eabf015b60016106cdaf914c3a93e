package core.model.test;

import java.io.Serializable;
import javax.persistence.*;


/**
 * The persistent class for the student database table.
 * 
 */
@Entity
@Table(name="student")
@NamedQuery(name="DefaultStudent.findAll", query="SELECT d FROM DefaultStudent d")
public class DefaultStudent implements Serializable, Student {
	private static final long serialVersionUID = 1L;

	@Id
	@GeneratedValue(strategy=GenerationType.IDENTITY)
	private int id;

	private String email;

	private int idDepartment;

	private int idDesk;

	private String name;

	public DefaultStudent() {
	}

	@Override
	public int getId() {
		return this.id;
	}

	@Override
	public void setId(int id) {
		this.id = id;
	}

	@Override
	public String getEmail() {
		return this.email;
	}

	@Override
	public void setEmail(String email) {
		this.email = email;
	}

	@Override
	public int getIdDepartment() {
		return this.idDepartment;
	}

	@Override
	public void setIdDepartment(int idDepartment) {
		this.idDepartment = idDepartment;
	}

	@Override
	public int getIdDesk() {
		return this.idDesk;
	}

	@Override
	public void setIdDesk(int idDesk) {
		this.idDesk = idDesk;
	}

	@Override
	public String getName() {
		return this.name;
	}

	@Override
	public void setName(String name) {
		this.name = name;
	}

}